source "https://rubygems.org"
gem "github-pages", group: :jekyll_plugins

group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.12"
  gem "jekyll-paginate", "~> 1.1"
  gem "jekyll-gist", "~> 1.5", :group => :jekyll_plugins
  gem "jekyll-seo-tag", "~> 2.7", :group => :jekyll_plugins
  gem "jekyll-sitemap", "~> 1.4", :group => :jekyll_plugins
  gem "jekyll-git_metadata", "~> 0.2.0", :group => :jekyll_plugins
  gem "disqus-for-jekyll", "~> 1.0", :group => :jekyll_plugins
end




